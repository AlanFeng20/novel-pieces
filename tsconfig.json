{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    // "allowSyntheticDefaultImports":true,
    "noImplicitAny": true,
    "removeComments": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "lib": [
      "es6",
      "ES2019.Array",
      "DOM"
    ]
  },
  "include": [
    "./src/**/*",
    "src/main.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}